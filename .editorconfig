root = true

[*]
charset = utf-8
insert_final_newline = true
trim_trailing_whitespace = true

[*.fs]
indent_size = 4
indent_style = space

[*.json]
indent_size = 2
indent_style = space

[*.{ts,js}]
indent_size = 2
indent_style = space

[*.liquid]
indent_size = 2
indent_style = space

[*.dbml]
indent_size = 2
indent_style = space

[*.{yml,yaml}]
indent_size = 2
indent_style = space
